{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://back-k1a3.onrender.com/api/$1",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "DELETE",
          "OPTIONS"
        ]
      },
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      }
    ]
  }
  